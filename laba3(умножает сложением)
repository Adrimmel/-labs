.data
    m1: .word 122      # Множимое
    m2: .word 7           # Множитель
    res: .word 0               # Результат

.text
.globl main

main:
    lw $t0, m1        # Загрузка множимого в регистр $t0
    lw $t1, m2          # Загрузка множителя в регистр $t1

    li $t2, 0                    # Обнуление регистра-счетчика
    li $t3, 0                    # Обнуление регистра-результата

loop:
    beq $t2, $t1, done           # Проверка условия завершения

    addu $t3, $t3, $t0           # Прибавление множимого к результату
    addiu $t2, $t2, 1            # Увеличение регистра-счетчика на 1
    j loop                       # Переход на следующую итерацию

done:
    sw $t3, res               # Сохранение результата

    # Вывод результата
    print_res:
        li $v0, 1                # Установка системного вызова для вывода целого числа
        lw $a0, res          # Загрузка значения результата в регистр $a0
        syscall                  # Выполнение системного вызова

    # Завершение программы
    exit:
        li $v0, 10               # Установка системного вызова для завершения программы
        syscall                  # Выполнение системного вызова
